# Copy to .env and edit as needed.
# UID/GID must match your host user so files in ./persist are owned correctly.
LOCAL_UID=1000
LOCAL_GID=1000
LOCAL_USER=dev

# AI selection
AI_PROVIDER=ollama
AI_MODEL=qwen2.5-coder:7b
AI_BASE_URL=http://ollama:11434

# If using OpenAI-compatible mode (via litellm or any OpenAI-like server)
OPENAI_COMPAT_BASE_URL=http://litellm:4000
OPENAI_COMPAT_API_KEY=

# Optional: Litellm remote providers (only if you want)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
